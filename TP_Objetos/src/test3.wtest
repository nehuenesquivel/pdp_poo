		libroDeHechizos.agregarHechizo(espectroMalefico)
		libroDeHechizos.agregarHechizo(hechizoBasico)
		
		rolando.hechizoPreferido(libroDeHechizos)
		assert.equals(56,rolando.nivelDeHechiceria())
	}
	
